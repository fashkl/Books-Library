<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mohamedmabrouk.filedata">

    <application
        android:allowBackup="true"
        android:icon="@drawable/library"
        android:label="Books Library"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LogInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NextActivity"
            android:label="Options"
            android:parentActivityName=".LogInActivity" />
        <activity
            android:name=".Show_All_Books_Activity"
            android:label="All Books"
            android:parentActivityName=".NextActivity" />
        <activity
            android:name=".Show_My_Books_Activity"
            android:label="My Books"
            android:parentActivityName=".NextActivity" />
    </application>

</manifest>
